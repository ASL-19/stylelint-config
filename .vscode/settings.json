{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "cSpell.words": [
    "Heaslip",
    "peerdeps",
    "stylelint"
  ],
  "search.exclude": {
    "CHANGELOG.markdown": true,
    "LICENSE": true,
    "package-lock.json": true
  },
}
