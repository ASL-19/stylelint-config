{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "cSpell.words": [
    "Heaslip",
    "destructure",
    "peerdeps",
    "stylelint"
  ],
  "search.exclude": {
    "CHANGELOG.markdown": true,
    "LICENSE": true,
    "package-lock.json": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
